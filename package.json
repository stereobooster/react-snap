{
  "name": "react-snap-typed",
  "version": "1.26.31",
  "description": "Zero-configuration framework-agnostic static prerendering for SPAs",
  "main": "dist/index.js",
  "bin": {
    "react-snap": "dist/run.js"
  },
  "files": [
    "dist/",
    "vendor/"
  ],
  "author": "stereobooster",
  "contributors": [
    "jan-rycko"
  ],
  "license": "MIT",
  "repository": "jan-rycko/react-snap",
  "engines": {
    "node": ">= 12.22.11"
  },
  "scripts": {
    "toc": "yarn run markdown-toc -i doc/recipes.md",
    "pretest": "npm run build",
    "test": "jest",
    "build": "tsc",
    "prepublish": "npm run build",
    "precommit": "prettier --write {*,src/*}.{js,json,css}",
    "force-resolutions": "npx --yes npm-force-resolutions"
  },
  "resolutions": {
    "node-fetch": "2.6.7",
    "nth-check": "2.0.1"
  },
  "dependencies": {
    "clean-css": "4.2.1",
    "express": "4.17.3",
    "express-history-api-fallback": "2.2.1",
    "html-minifier": "4.0.0",
    "minimalcss": "0.8.3",
    "mkdirp": "^1.0.4",
    "puppeteer": "^13.5.1",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-block-resources": "^2.4.2",
    "serve-static": "1.14.1",
    "setimmediate": "^1.0.5",
    "shelljs": "^0.8.5",
    "sourcemapped-stacktrace-node": "2.1.8"
  },
  "devDependencies": {
    "@types/clean-css": "^4.2.5",
    "@types/express": "^4.17.13",
    "@types/html-minifier-terser": "^6.1.0",
    "@types/jest": "^27.4.1",
    "@types/shelljs": "^0.8.11",
    "dev-null-stream": "0.0.1",
    "jest": "^27.5.1",
    "markdown-toc": "1.2.0",
    "prettier": "1.18.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
